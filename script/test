#!/usr/bin/env bash

ROOT="`pwd`"
HOME=~

if [[ $TRAVIS = 'true' ]]; then
    PATH_REPO=$ROOT
fi

"$ROOT/bootstrap.sh"

[ -r "$ROOT/src/load.sh" ] && source "$ROOT/src/load.sh"

for path in "$HOME/".{pyenv,gvm,tmux,fonts}; do
    if [[ ! -r $path ]]; then
        error $path
    fi
done
unset path

cat <<EOF

--------------------------
    Test Executed
--------------------------

EOF
