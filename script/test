#!/usr/bin/env bash

ROOT="`pwd`"
HOME=~

"$ROOT/bootstrap.sh"

[ -r "$ROOT/src/load.sh" ] && source "$ROOT/src/load.sh"

for path in "$HOME/".{pyenv,gvm,tmux,fonts}; do
    ret="0"
    if [[ ! -r $path ]]; then
        ret="1"
        error $path
    else
        success $path
    fi
done
unset path

cat <<EOF

--------------------------
    Test Executed
--------------------------

EOF
