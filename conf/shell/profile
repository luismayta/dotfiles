#!/usr/bin/env bash
# -*- coding: utf-8 -*-

# Load the shell dotfiles, and then some:
# * ~/.customrc can be used for other settings you donâ€™t want to commit.
# shellcheck source=/dev/null
for file in ~/.{exports,bash_prompt,load,aliases,functions,inputrc,customrc}; do
	[ -r "$file" ] && source "$file"
done
unset file
