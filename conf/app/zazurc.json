{
  "displayOn": "detect",
  "hotkey": "ctrl+space",
  "plugins": [
    "tinytacoteam/zazu-calculator",
    "tinytacoteam/zazu-template",
    "tinytacoteam/zazu-system",
    "tinytacoteam/zazu-package-manager",
    "fracturedloop/zazu-dash",
    "blainesch/zazu-timer",
    "danielbayerlein/zazu-devdocs",
    "chrishelgert/zazu-fkill",
    {
      "name": "tinytacoteam/zazu-fallback",
      "variables": {
        "rootSearches": ["duck", "stack", "giphy", "gh", "google", "amazon"]
      }
    },
    {
      "name": "tinytacoteam/zazu-clipboard",
      "variables": {
        "ClipboardKey": "cmd+shift+v"
      }
    },
    {
      "name": "tinytacoteam/zazu-file-finder",
      "variables": {
        "append": true,
        "directories": {
          "appPath": ["/Applications"],
          "excludeName": ["node_modules", "*.scpt"],
          "excludePath": ["~/Library"],
          "filePath": ["~/Documents", "~/Downloads", "~/Dropbox"]
        }
      }
    },
    "sagold/zazu-dict",
    "nantaphop/zazu-docker",
    "twang2218/zazu-tldr",
    "halvves/zazu-giphy"
  ],
  "theme": "iansinnott/zazu-dark-theme"
}
